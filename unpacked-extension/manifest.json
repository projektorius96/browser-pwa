{
    "name": "browser window appbar controls",
    "version": "0.1.1",
    "manifest_version": 3,
    "description": "The extension provides an overlay to OS-level browser's native appbar controls",
    "icons": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "background": {
        "service_worker": "service-worker.js"
    },
    "externally_connectable": {
        "matches": ["<all_urls>"]
    },
    "permissions": ["tabs"]
}